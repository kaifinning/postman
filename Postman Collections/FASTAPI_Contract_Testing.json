      "_postman_id": "a7e0f45f-a001-4f09-bae1-a868505bafec",
      "name": "TestingLoop",
      "event": [
        {
          "listen": "test",
          "script": {
            "id": "3fb9e853-f182-4170-b6c0-d7fb5d71887a",
            "type": "text/javascript",
            "exec": [
              "//testData = ['May20', 'May20th', '05-20-2018', '2018-05-20']",
              "//var testData = JSON.parse(pm.environment.get(\"testData\"));",
              "var testData1 = pm.variables.get(\"testData\");",
              "console.log(testData1);",
              "//pm.environment.unset(\"endDate\");",
              "",
              "for(var i=0; i<=testData1.length; i++){",
              "    //pm.environment.unset(\"endDate\");",
              "    pm.environment.set(\"endDate\", testData1[i]);",
              "    console.log(testData1[i]);",
              "    postman.setNextRequest(\"TestingLoop\");",
              "    console.log(request.url);",
              "    pm.test(\"Status code is 200\", function () {",
              "        pm.response.to.have.status(200);",
              "    });",
              "    testData1.splice(testData1[i], 1);",
              "    pm.environment.unset(\"endDate\");",
              "    console.log(testData1);",
              "}",
              "//;",
              "",
              "",
              "",
              ""
            ]
          }
        },
        {
          "listen": "prerequest",
          "script": {
            "id": "9bef5b8d-3193-47f8-864d-814caa52a50b",
            "type": "text/javascript",
